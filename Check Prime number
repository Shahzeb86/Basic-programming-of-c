// write a program to check a prime number.
#include <stdio.h>
int main()
{
    int n,i;
    printf("Enter the number\n");
    scanf("%d",&n);
   if(n==1||n==2)
   {
       printf("This is prime number");
   }
  for(i=2;i<n;i++)
  {
       if(n%i==0)
       {
           printf("This is not prime  number");
           break;
       }
  }
  if(i==n)
  {
      printf("This is prime number");
  }
    return 0;
}
